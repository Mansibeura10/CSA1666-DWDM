# Load the "AirPassengers" dataset
data("AirPassengers")

# Convert the dataset to a numeric vector
passenger_counts <- as.numeric(AirPassengers)

# Define the breaks for the histogram
breaks <- seq(100, 700, by = 150)

# Create the histogram
hist(passenger_counts, breaks = breaks, main = "Histogram of AirPassengers Dataset",
     xlab = "Number of Passengers", ylab = "Frequency", col = "lightblue", border = "black")
